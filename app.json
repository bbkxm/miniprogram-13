{
  "pages": ["pages/index/index", "pages/publishActivity/index", "pages/map/index", "pages/profile/index"],
  "usingComponents": {
    "t-toast": "tdesign-miniprogram/toast/toast"
  },
  "subpackages": [
    {
      "root": "pages/myActivities",
      "name": "myActivities",
      "pages": ["index"]
    },
    {
      "root": "pages/activity",
      "name": "activity",
      "pages": ["index"]
    },
    {
        "root": "pages/checkin",
        "name": "checkin",
        "pages": ["index"]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "Weixin",
    "navigationBarTextStyle": "black"
  },
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "活动广场"
      },
      {
        "pagePath": "pages/map/index",
        "text": "打卡地图"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的"
      }
    ]
  },
  "resolveAlias": {
    "~/*": "/*"
  },
  "sitemapLocation": "sitemap.json",
  "requiredPrivateInfos": [
    "getLocation",
    "chooseLocation"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  }
}
